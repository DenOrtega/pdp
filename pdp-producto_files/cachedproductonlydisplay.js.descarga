						var ceselectedStore = cestoresDropDown.options[cestoresDropDown.selectedIndex].value;	
						if(ceselectedStore == "1"){
							dojo.byId('physicalCERegionErr').style.display = "block";
							return false;
						}
					}
					//zona end
