			} 
			else {
				 if (serviceResponse.errorMessageKey) {
					MessageHelper.displayErrorMessage(serviceResponse.errorMessageKey);
				 }
			}
			cursor_clear();
			
			//MTHN: Start Req# INT1875 : Create Update order OEM			
			dojo.publish("ajaxOrderCalculateComplete");
			
